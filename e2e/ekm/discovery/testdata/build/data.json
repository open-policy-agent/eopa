{
  "discovery": {
    "service": "acmecorp",
    "resource": "discovery.tar.gz",
    "signing": {
      "keyid": "rsa"
    }
  },
  "services": {
    "acmecorp": {
    }
  },
  "plugins": {
    "data": {
      "from.http": {
        "url": "http://127.0.0.1:9999/test",
        "type": "http",
        "headers": {
          "Authorization": "Bearer ${vault(kv/data/plugin/header:data/value)}"
        }
      }
    }
  },
  "ekm": {
    "vault": {
      "access_type": "token",
      "httpsend": {
        "http://127.0.0.1:9999": {
          "headers": {
            "Authorization": {
              "bearer": "kv/data/httpsend/bearer:data/token",
              "scheme": "kv/data/httpsend/bearer:data/scheme"
            }
          }
        }
      },
      "license": {
        "key": "kv/data/license:data/key"
      },
      "services" : {
        "acmecorp.credentials.bearer.token": "kv/data/acmecorp/bearer:data/token",
        "acmecorp.credentials.bearer.scheme": "kv/data/acmecorp/bearer:data/scheme",
        "acmecorp.url": "kv/data/acmecorp:data/url"
      },
      "keys" : {
        "rsa.key": "kv/data/discovery/rsa:data/key"
      }
    }
  }
}

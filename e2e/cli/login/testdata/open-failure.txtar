[windows] skip 'no way to check this on windows'

env PATH=$WORK:$PATH
chmod 755 $WORK/open
chmod 755 $WORK/xdg-open
! exec eopa login --timeout 0.1s
stderr 'Could not open browser \(exit status 1\)\.'
stderr 'Please navigate to the following URL in your browser:'
stderr '  https://my\.styra\.com/cli-sign-in\?callback=[0-9]+'
stderr 'Error: context deadline exceeded'

-- .styra.yaml --
url: https://my.styra.com

-- open --
#!/bin/sh
exit 1

-- xdg-open --
#!/bin/sh
exit 1

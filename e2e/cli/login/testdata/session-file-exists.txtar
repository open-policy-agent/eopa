# file already exists, updates perms
chmod 777 .styra-session
stdin pasted-token
exec eopa login --read-token
stderr 'Successfully stored token'
cmp .styra-session pasted-token
exec ls -l .styra-session
[!windows] stdout '-rwx------'
[windows]  stdout '-rw-r--r--'

-- pasted-token --
uninteresting

-- .styra-session --
old token

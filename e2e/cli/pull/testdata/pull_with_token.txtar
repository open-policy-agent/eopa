# the "token" file is only used for syncmock
! exec syncmock --port 9994 --token-file token &
[darwin] exec sleep 1

env EOPA_STYRA_DAS_TOKEN=opensesame
exec eopa pull --log-level warn
! stdout .
! stderr .
exec diff -r exp .styra/include

-- token --
opensesame

-- .styra.yaml --
url: http://127.0.0.1:9994

-- exp/.gitignore --
*
-- exp/libraries/lib02/envoy/policy.rego --
package libraries.lib02.envoy
import future.keywords
allow if true

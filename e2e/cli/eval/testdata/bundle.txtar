exec eopa eval -f pretty --bundle toplevel data
! stderr .
cmp stdout exp-toplevel/stdout

exec eopa eval -f pretty --bundle nested data
! stderr .
cmp stdout exp-nested/stdout

-- toplevel/data.yaml --
/some-yaml:
  goes:
    here: NO
-- nested/a/b/c/data.yaml --
yes: no
